<odoo>
  <data>

    <record id="edit_project_inherit_project_iht" model="ir.ui.view">
      <field name="name">project.project.view.form.inherit.project_iht</field>
      <field name="model">project.project</field>
      <field name="inherit_id" ref="project.edit_project"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='user_id']" position="after">
          <field name="lokasi_proyek"/>
          <field name="source_aplikasi_pendukung" widget="url"/>
          <field name="daftar_pekerja_remote"/>
        </xpath>
      </field>
    </record>

  </data>
</odoo>
